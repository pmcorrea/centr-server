		"jsonwebtoken": "^8.5.1",
		"knex": "^0.20.7",
		"morgan": "^1.9.1",
		"pg": "^7.17.1",
		"xss": "^1.0.6"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"mocha": "^6.2.2",
		"nodemon": "^2.0.2",
		"supertest": "^4.0.2",
		"postgrator-cli": "^3.2.0"
